import java.util.Scanner;

public class LeapYearChecker {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Введіть рік (1900-3000): ");

        if (scanner.hasNextInt()) {
            int year = scanner.nextInt();

            // Перевірка діапазону років
            if (year >= 1900 && year <= 3000) {
                // Логіка визначення високосного року
                // (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0)
                boolean isLeap = (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);

                if (isLeap) {
                    System.out.println("**Високосний**");
                } else {
                    System.out.println("**Звичайний**");
                }
            } else {
                System.out.println("Рік має бути в діапазоні від 1900 до 3000.");
            }
        } else {
            System.out.println("Невірний ввід. Будь ласка, введіть ціле число.");
        }
        scanner.close();
    }
}
